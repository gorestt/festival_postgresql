APP_PORT=3000

# PostgreSQL connection (application user)
PGHOST=localhost
PGPORT=5432
PGDATABASE=festivaldb
PGUSER=festival_app
PGPASSWORD=festival_app

# Provisioning: admin credentials used to create DB/user automatically via scripts/run files
